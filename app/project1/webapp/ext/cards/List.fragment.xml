<core:FragmentDefinition 	
    xmlns="sap.suite.ui.commons"
    xmlns:ovp="sap.ovp.ui"
	xmlns:m="sap.m"
    xmlns:smartchart="sap.ui.comp.smartchart"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:layout="sap.ui.layout"
	xmlns:core="sap.ui.core"
    xmlns:data="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1">
	<!-- Here you can put your Card's Content Area -->
    <m:FlexBox height="500px">
    <smartchart:SmartChart enableAutoBinding="true"
        entitySet="MaintOrdersAnalytics" useVariantManagement="false"
        persistencyKey="PKeyChartExample9" useChartPersonalisation="true"
        showFullScreenButton="true" selectionMode="Multi"
        showChartTooltip="true" showDrillBreadcrumbs="false"
        showDetailsButton="false" showDrillButtons="true" height="100%"
        showSemanticNavigationButton="false"
        >
    </smartchart:SmartChart>
    </m:FlexBox>
    
</core:FragmentDefinition>