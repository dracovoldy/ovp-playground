{
    "_version": "1.32.0",
    "sap.app": {
        "id": "project1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "dataSources": {
            "mainService": {
                "uri": "/v2/analytics/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "annotations": [
                        "annotation",
                        "annotation0"
                    ],
                    "localUri": "localService/metadata.xml"
                }
            },
            "catalogService": {
                "uri": "/v2/catalog/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            },
            "annotation0": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation0.xml",
                "settings": {
                    "localUri": "annotations/annotation0.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "OVP.smartovptemplate",
            "version": "1.41.1"
        },
        "crossNavigation": {
            "inbounds": {
                "project1-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "MyOvp",
                    "action": "monitor",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.97.2",
            "libs": {
                "sap.ovp": {}
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties",
                "preload": false
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "mainService": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultCountMode": "Inline"
                }
            },
            "catalogService": {
                "dataSource": "catalogService",
                "preload": true,
                "settings": {
                    "defaultCountMode": "Inline"
                }
            }
        },
        "extends": {
            "extensions": {}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    },
    "sap.ovp": {
        "globalFilterModel": "mainService",
        "globalFilterEntityType": "MaintOrders",
        "containerLayout": "resizable",
        "enableLiveFilter": true,
        "considerAnalyticalParameters": false,
        "chartSettings": {
            "showDataLabel": true
        },
        "cards": {
            "card01": {
                "model": "catalogService",
                "template": "project1.cards.analytics",
                "settings": {
                    "title": "{{card01_title}}",
                    "entitySet": "WorkOrderAnalytics1",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#OrdersByWorkCenter",
                    "presentationVariantAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#OrdersByWorkCenterPreVar",
                    "defaultSpan": {
                        "rows": 1,
                        "cols": 1,
                        "showOnlyHeader": false
                    },
                    "custom": {
                        "height": 465
                    }
                }
            },
            "card00": {
                "model": "catalogService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "{{card00_title}}",
                    "subTitle": "{{card00_subtitle}}",
                    "entitySet": "WorkOrderAnalytics1",
                    "navigation": "noHeaderNav",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#OrdersByWorkCenter",
                    "presentationAnnotationPath" : "com.sap.vocabularies.UI.v1.PresentationVariant#OrdersByWorkCenterPreVar",
                    "stopResizing": false,
                    "defaultSpan": {
                        "rows": 1,
                        "cols": 1,
                        "showOnlyHeader": false
                    }
                }
            }
        }
    }
}