{
	"name": "zpm-analytics",
	"version": "0.0.1",
	"description": "A Fiori application.",
	"keywords": [
		"ui5",
		"openui5",
		"sapui5"
	],
	"main": "webapp/index.html",
	"scripts": {
		"deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
		"start": "fiori run --open 'test/flpSandbox.html#dcorechevronzpmanalytics-display'",
		"start-local": "fiori run --config ./ui5-local.yaml --open 'test/flpSandboxMockServer.html#dcorechevronzpmanalytics-display'",
		"start-noflp": "fiori run --open 'index.html'",
		"start-variants-management": "fiori run --open \"preview.html?fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
		"build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
		"deploy": "fiori verify",
		"start-mock": "fiori run --open 'test/flpSandboxMockServer.html#dcorechevronzpmanalytics-display'"
	},
	"devDependencies": {
		"@ui5/cli": "^2.14.1",
		"@ui5/fs": "^2.0.6",
		"@ui5/logger": "^2.0.1",
		"@sap/ux-ui5-tooling": "1",
		"rimraf": "3.0.2",
		"@sap/ux-specification": "latest"
	},
	"ui5": {
		"dependencies": [
			"@sap/ux-ui5-tooling"
		]
	}
}
