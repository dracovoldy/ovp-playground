          "@sap.updatable": "false"
        },
        "LeanServiceDuration": {
          "type": "cds.Decimal",
          "precision": 5,
          "scale": 1,
          "doc": "Specifies the duration of the lean service that can differ from the duration of the order operation.\n\nIf you add a lean service to an order operation and specify the duration of the lean service, the system uses the duration as a basis to calculate the start date and time and the end date and time of the service. If an offset is defined for the lean service, the system considers this also. How the service start date and end date is calculated depending on your entries in the Align Requirement Date table column is described below.The system calculates the service start and end dates and times based on the service duration that you specify. How these dates are calculated depends on your entry in the Align Requirement Date table column:If you add a new lean service to the order operation and do not change the default entry, the start date and time of the service is aligned to the earliest start date and time of the operation. The system calculates the end date and time of the lean service by adding the service duration to the start date.If you select Align to Finish Date, the end date and time of the service is aligned to the latest end date and time of the operation. The system calculates the start date and time of the service backwards, based on the service duration that you specify.If you select Manual Requirement Date, you can specify the start and end date and time of the lean service manually. In this case, you cannot specify the duration in addition.If you define an offset in the Lead Time Offset table column, the system considers this offset in its time calculation.",
          "@sap.unit": "LeanServiceDurationUnit",
          "@sap.label": "Service Duration",
          "@sap.quickinfo": "Duration of Lean Service"
        },
        "LeanServiceDurationUnit": {
          "type": "cds.String",
          "length": 3,
          "doc": "Indicates the time unit in which the duration of the lean service is stated.\n\nIf you have specified the duration of the order operation in a different time unit, the system converts the time unit to calculate the end date and time of the lean service.",
          "@sap.label": "Serv. Dur. Unit",
          "@sap.quickinfo": "Duration Unit for a Lean Service",
          "@sap.semantics": "unit-of-measure"
        },
        "DistributionFunction": {
          "type": "cds.String",
          "length": 3,
          "doc": "Key determining the distribution function (equal distribution over all periods, etc.) with which you want to dispatch the capacity requirements.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Function",
          "@sap.quickinfo": "Distribution function"
        },
        "SrvcSchedgIsAlignedWthOpWrkCtr": {
          "type": "cds.Boolean",
          "doc": "Controls whether the scheduling of a lean service is aligned with the available capacity of the work center.\n\nIf you choose to calculate the end date and time or the start date and time of a lean service automatically by selecting the corresponding option in the Align Requirement Date table column, you can select this checkbox to align the scheduling with the available capacity of the work center. This ensures that the lean service can be performed within the schedule of the work center.If you select this checkbox and choose the option Align to Start Date, the system aligns the start date and time of the lean service to the earliest start date and time of the operation. If you choose the option Align to Finish Date, it aligns the end date and time of the lean service to the latest end date and time of the operation. When calculating the end date and time or the start date and time of the lean service, the system distributes the duration in such a way that it coincides fully with the available capacity of the work center.This checkbox is disabled if you choose the option Manual Requirement Date in the Align Requirement Date table column.You enter a lean service and choose Align to Start Date. You specify a service duration of 12 hours. The earliest start date and time of the operation is on July 27 at 6:00 AM. The available capacity of the work center is from 9:00 AM to 3:00 PM with a 30-minute break. This means that the work center has a capacity of 5.5 hours per day.If you do not select this checkbox, the lean service will be scheduled to start on July 27 at 6:00 AM. It will end 12 hours later, on July 27 at 6:00 PM. In this case, only 5.5 hours of the entire service duration coincide with the work center capacity.If you select this checkbox, the lean service will be scheduled to start on July 27 at 6 AM. However, it will end on July 29 at 10 AM. In this case, the service duration will be distributed over several days and therefore coincide fully with the work center capacity.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Align with Work Center",
          "@sap.quickinfo": "Align scheduling with work center operating hours"
        },
        "MaintOrderCompDebitCreditCode": {
          "type": "cds.String",
          "length": 1,
          "doc": "Shows on which side of the account (S = debit, H = credit) the transaction figures are updated.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Debit/Credit ind",
          "@sap.quickinfo": "Debit/Credit Indicator"
        },
        "GoodsMovementIsAllowed": {
          "type": "cds.Boolean",
          "doc": "Specifies that goods movements are allowed for the reservation item.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Movement Allowed",
          "@sap.quickinfo": "Goods Movement for Reservation Allowed"
        },
        "MaintenanceOrderComponentBatch": {
          "type": "cds.String",
          "length": 10,
          "doc": "Assigns a material that is manufactured in batches or production lots to a specific batch.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Batch",
          "@sap.quickinfo": "Batch Number"
        },
        "QuantityIsFixed": {
          "type": "cds.Boolean",
          "doc": "Determines that the quantity of an item is constant.\n\nSet this indicator if the quantity in a relational system does not change.If an item of a BOM is marked as being fixed and the quantity of the finished product to be manufactured is changed, the quantity of this item does not change.If a &quot;materials to be provided&quot; item is not marked as being fixed, and the quantity of the subcontracted item is changed, the quantity of the material to be provided to the subcontractor changes proportionately.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Quantity is Fixed"
        },
        "MaintOrdOpComponentGLAccount": {
          "type": "cds.String",
          "length": 10,
          "doc": "The G/L account number identifies the G/L account in a chart of accounts.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "G/L Account",
          "@sap.quickinfo": "G/L Account Number"
        },
        "MaintOrdOpCompCostingRelevancy": {
          "type": "cds.String",
          "length": 1,
          "doc": "Controls the extent to which a BOM item, operation, or suboperation in the routing is included in costing.\n\nFor the Standard Cost Estimate and the calculation of planned costs for a Work Order, this indicator specifies whether the item is included in costing.A BOM item or operation for which the indicator for relevancy to costing is X is fully relevant to costing.A BOM item or operation without an indicator for relevancy to costing is not relevant to costing at all.For Inventory Costing, you can link relevancy to costing indicators to factors for the fixed and variable costs so that the item values can be adjusted in costing depending on the valuation variant. You do this in Customizing for Product Cost Controlling.A BOM item or operation has the indicator for relevancy to costing A, which you have linked in Customizing for Product Cost Controlling to a fixed factor of 0.8 and a variable factor of 0.8. The BOM item or operation is costed in inventory costing at 80% of the fixed costs and 80% of the variable costs.For output materials (that is, materials for which the co-product indicator has been set in the material master and that have a negative BOM item), the relevance to costing indicator has no effect on the BOM item.The BOM usage that you selected on the initial screen determines whether this field is ready for input. For more information, see Customizing under Production -> Basic Data -> Bill of Material -> General Data -> BOM Usage -> Define BOM Usages.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "CostingRelevncy",
          "@sap.quickinfo": "Indicator for Relevancy to Costing"
        },
        "MaintCompAltvProdUsgeRateInPct": {
          "type": "cds.Decimal",
          "precision": 3,
          "scale": 0,
          "doc": "Percentage probability that a material will be used in the assembly.\n\nThe usage probability is always taken into account for requirements planning.Maintain selected usage probabilityRequirements planning for alternative items is based on the usage probability.Quantities are suggested based on the usage probability for the withdrawal posting according to strategy 1. You can plan in excess for the alternative items since the usage probability is not limited to 100%. You can change the proposed quantities. Maintain no usage probability If you do not enter a usage probability, the alternative item just serves as information that can be evaluated, for example, in a situation with a missing part.You receive a warning since dependent requirements with 0 as quantity are created.Withdrawal based on usage probabilityStrategy: value 1An alternative item group is maintained for each assembly. This alternative item group contains four materials with differing usage probabilities. Dependent requirements are passed on with 100 as quantity.Material Usage probability (%) RequirementM-1 70 70M-2 20 20M-3 20 20M-4 0 no requirementIn this case, the materials are planned in excess since the sum of the usage probabilities is greater than 100%.",
          "@sap.label": "Usage Probability",
          "@sap.quickinfo": "Usage Probability in % (Alternative Item)",
          "@sap.creatable": "false",
          "@sap.updatable": "false"
        },
        "MaintOrderOpComponentSortText": {
          "type": "cds.String",
          "length": 10,
          "doc": "String which can be defined as required for sorting components for display.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Sort String"
        },
        "MaintOrdOpCompIsBulkProduct": {
          "type": "cds.Boolean",
          "doc": "This indicator defines a BOM item as a bulk material, which is available directly at the work center (for example, washers or grease).\n\nThe dependent requirements of bulk materials are not relevant to materials planning. This means they are not taken into account for net requirements calculation. Therefore planning for bulk materials should be consumption-based.If bulk materials are subject to material requirements planning, you must create planned independent requirements for them.Dependent requirements are only created in the planned order for information purposes and they can be displayed there. Dependent requirements for bulk material are not shown in the MRP list or the stock/req. list.You can maintain this indicator in both the material master record an the BOM item. The indicator in the material master record has higher priority.If a material is always used as a bulk material, set the indicator in the material master record.In this case, the Bulk material indicator in material master record is automatically set in the BOM item.If a material is only used as a bulk material in individual cases, set the indicator in the BOM item.To improve system performance, you can define in the IMG for MRP that no dependent requirements are to be created for bulk material items. However, this also means that you see no information on bulk material components in the planned order or production order.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Bulk Material",
          "@sap.quickinfo": "Indicator: Bulk Material"
        },
        "MaterialProvisionType": {
          "type": "cds.String",
          "length": 1,
          "doc": "This key indicates that the item is subject to material provision.\n\nIn the BOM explosion, you can use this indicator to restrict the items selected.There are two types of material provision:Material provided by a vendorMaterial provided by a customerYou can also use the material provision indicator to identify the subcontracting type for a part. The following indicators are available:Refurbishment material to SC (value S in standard SAP system)Refurbishment material from SC (value X in standard SAP system)Subsequent delivery (value F in standard SAP system)If you do not enter information in this field, the components are provided to the subcontractor as consumable materials (initial value in the standard SAP system).Material provided by a vendorThis indicator controls material requirements planning. Maintain this indicator for BOM components only if the material master record of the header material supports subcontracting (Special procurement field).The special procurement key for subcontracting means that all components of the BOM are provided free of charge to a subcontractor for further processing. The items are copied to a subcontract order.If the item is provided to a vendor, do not enter any material provision indicator.Dependent requirements are generated for this item.If the item is already at a vendor's premises and is not provided by us, do enter an indicator.No dependent requirements are generated for this item.Material provided by a customerThis indicator is not currently active.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Mat. Provision Ind.",
          "@sap.quickinfo": "Material Provision Indicator"
        },
        "MaintOrdOpCompAssgdWBSElmntInt": {
          "type": "cds.String",
          "length": 8,
          "doc": "Key identifying a WBS element.",
          "@sap.display.format": "NonNegative",
          "@sap.label": "WBS Internal ID",
          "@sap.quickinfo": "WBS Element"
        },
        "MaintOrderOpComponentPrice": {
          "type": "cds.Decimal",
          "precision": 15,
          "scale": 3,
          "@sap.unit": "MaintOrdOpComponentCurrency",
          "@sap.variable.scale": "true",
          "@sap.label": "Price/currency",
          "@sap.quickinfo": "Price in component currency"
        },
        "MaintOrdOpComponentCurrency": {
          "type": "cds.String",
          "length": 5,
          "doc": "Currency key for amounts in the system.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Currency",
          "@sap.quickinfo": "Currency Key",
          "@sap.semantics": "currency-code"
        },
        "MatlCompIsMarkedForBackflush": {
          "type": "cds.Boolean",
          "doc": "Indicator specifying that the material component is backflushed.\n\nEach material component is assigned to an operation in the production order. If a material component is backflushed, the system posts the withdrawal only uponconfirmation of the operation. The withdrawal is then posted automatically.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Backflush",
          "@sap.quickinfo": "Indicator: Backflush"
        },
        "PurchasingGroup": {
          "type": "cds.String",
          "length": 3,
          "doc": "Key for a buyer or a group of buyers, who is/are responsible for certain purchasing activities.\n\nInternally, the purchasing group is responsible for the procurement of a material or a class of materials.Externally, it is the medium through which contacts with the vendor are maintained.",
          "@sap.display.format": "UpperCase",
          "@sap.label": "Purchasing Group"
        },
        "DeliveryTimeInDays": {
          "type": "cds.Decimal",
